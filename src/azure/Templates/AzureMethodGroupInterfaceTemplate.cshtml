@using System
@using AutoRest.Java.azure.Templates
@using AutoRest.Java.vanilla.Templates
@using System.Linq;
@using AutoRest.Core.Utilities;
@using AutoRest.Java
@using AutoRest.Java.Model
@using AutoRest.Java.Azure.Model
@inherits AutoRest.Core.Template<AutoRest.Java.Azure.Model.MethodGroupJva>
/**
@Header(" * ").TrimMultilineHeader()
 */
@EmptyLine

package @(Settings.Namespace.ToLower());
@EmptyLine
@foreach (MethodJv m in Model.Methods)
{
@if (m.Extensions.ContainsKey("OptionalParameterClassName"))
{
@:import @($"{Settings.Namespace.ToLower()}.models.{(string)m.Extensions["OptionalParameterClassName"]};")
}
}

@foreach (var importClass in Model.InterfaceImports)
{
@:import @importClass;
}
@EmptyLine

/**
 * An instance of this class provides access to all the operations defined
 * in @(Model.TypeName).
 */
public interface @Model.TypeName {
    @foreach (MethodJv method in Model.Methods)
    {
    @if (method.Extensions.ContainsKey("OptionalParameterClassName"))
    {
    @:@Include(new OptionalParamMethodInterfaceTemplate(), method)
    }
    else
    {
    @:@Include(new MethodInterfaceTemplate(), method)
    }
    @EmptyLine
    }
}